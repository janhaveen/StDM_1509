<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<style type="text/css">
dl{
	margin-bottom: 0rem	!important;	
}
</style>
</head>
<body>
<dl class="row">
	<dl class="col-sm-8 ">
		<dl class="row">
		  <dt class="col-sm-3">GRN Number</dt>
		  <dd class="col-sm-3" id="GRNNumberTextView">-</dd>
		  <dt class="col-sm-3">Vendor</dt>
		  <dd class="col-sm-3" id="VendorTextView">-</dd>
		</dl>
		<dl class="row">
			<dt class="col-sm-3">Purchase Date</dt>
		  	<dd class="col-sm-3" id="PurchaseDateTextView">-</dd>
			<dt class="col-sm-3">Contact Person</dt>
			<dd class="col-sm-3" id="ContactPersonTextView">-</dd>
		</dl>
	</dl>
	<dl class="col-sm-4">
		<dl class="row">
		  <dt class="col-sm-3">Address</dt>
		  <dd class="col-sm-9" id="AddressTextView">-</dd>
		 </dl>
	</dl>
</dl>
<dl class="row">
	<dl class="col-sm-12">
		<dl class="row">
		  <dt class="col-sm-2">Received By</dt>
		  <dd class="col-sm-2" id="ReceivedByTextView">-</dd>
		  <dt class="col-sm-2">Receipt Date</dt>
		  <dd class="col-sm-2" id="ReceiptDateTextView">-</dd>
		</dl>
		<dl class="row">
			<dt class="col-sm-2">Courier Name</dt>
		  	<dd class="col-sm-2" id="CourierNameTextView">-</dd>
			<dt class="col-sm-2">LR/Tracking Details</dt>
			<dd class="col-sm-2" id="LRTextView">-</dd>
			<dt class="col-sm-2">Courier Charges</dt>
			<dd class="col-sm-2" id="OtherTextView">-</dd>
		</dl>
	</dl>
</dl>
</body>
</html>